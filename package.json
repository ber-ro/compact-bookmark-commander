{
  "scripts": {
    "precreate": "npm run build",
    "create": "cd prod && powershell Compress-Archive -force * chrome.zip",
    "build": "babel src --extensions .tsx --out-dir prod/app && node build.mjs",
    "build:dev": "babel src --extensions .tsx --out-dir dev/app && node build.mjs --dev"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "import-rename",
        {
          "^(react-bootstrap).*": "../lib/$1.min.js",
          "^(react|react-dom).*$": "../lib/$1.js"
        }
      ]
    ],
    "targets": "last 2 Chrome versions"
  },
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@types/chrome": "^0.0.228",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "bootstrap": "^5.2.3",
    "cpy": "^9.0.1",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.32.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "requirejs": "^2.3.6"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-import-rename": "^1.0.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react-hooks": "^4.6.0"
  }
}
