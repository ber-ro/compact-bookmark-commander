{
  "scripts": {
    "precreate":"npm run build",
    "create":"cd prod && powershell Compress-Archive -force * chrome.zip",
    "build": "babel src --extensions .tsx --out-dir prod/app && node build.mjs",
    "build:dev": "babel src --extensions .tsx --out-dir dev/app && node build.mjs --dev"
  },
  "babel":{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [ "@babel/plugin-transform-modules-amd" ],
    "targets": "last 2 Chrome versions"
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@types/chrome": "^0.0.188",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "bootstrap": "^5.1.3",
    "cpy": "^9.0.1",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.1",
    "react": "^18.1.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.1.0",
    "requirejs": "^2.3.6"
  },
  "license":"MIT"
}
