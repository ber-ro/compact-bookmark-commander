{
  "name": "Compact Bookmark Commander",
  "description": "Show 2 independent bookmark folders side by side allowing file-like operations: move, delete, create folder, sort, ...",
  "version": "0.1.1",
  "manifest_version": 3,
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "host_permissions": [
    "https://www.google.com/s2/favicons?domain=",
    "https://*.gstatic.com/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "icons": {
    "128": "icon128.png"
  },
  "externally_connectable": {
    "ids": []
  },
  "content_security_policy": {
    "default-src": "self",
    "frame-ancestors": "none"
  }
}